public class codefile{
    static boolean check(String s){
		s = s.toLowerCase();
            Set<Character> letters = new HashSet<>(); 

            for (char ch : s.toCharArray())
            {
                  if (ch>= 'a' && ch<='z')
                  {
                        letters.add(ch);
                  }
            }
            return letters.size() == 26;
    }
}
