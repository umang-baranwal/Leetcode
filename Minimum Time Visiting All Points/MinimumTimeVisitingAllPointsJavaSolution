public class codefile{
    static int solve(List<List<Integer>>  input){
        int time =0;
        for (int i =1; i < input.size(); i++)
        {
          int x1 = input.get(i-1).get(0);
          int x2 = input.get(i).get(0);
          int y1 = input.get(i-1).get(1);
          int y2 = input.get(i).get(1);

          int dx = Math.abs(x2-x1);
          int dy = Math.abs(y2-y1);

          time+= Math.max(dy, dx);
        }
        return time;
    }
}
